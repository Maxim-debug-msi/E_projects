#include <iostream>
#include <vector>

void sort(std::vector <int> &vec){
    int tmp;
    for(int i = 0; i < vec.size() - 1; i++){
        for(int j = 0; j < vec.size() - i - 1; j++){
            if(vec[j] > vec[j + 1]){
                tmp = vec[j];
                vec[j] = vec[j + 1];
                vec[j + 1] = tmp;
            }
        }
    }
}

int main() {
    std::vector <int> vec;
    int num;
    std::cout<<"Populate the array. Press '-1' to display the 5th oldest item, press -2 to exit."<<std::endl;
    do{
        std::cin>>num;
        if(num != -1 && num != -2) vec.push_back(num);
        else sort(vec);//sorting an array
        if(num == -1 && vec.size() > 5) std::cout<<vec[4]<<std::endl;
        else if(num == -1) std::cout<<"Array with less than 5 elements."<<std::endl;
    }while(num != -2);

    for(int j = 0; j < vec.size(); j++){
        std::cout<<vec[j]<<" ";
    }
    return 0;
}
